    <div class="panel-table collapse">
        <table class="table">
            {% for name, test in tests %}
            <tr>
                <td width="150">
                    <div class="progress">
                        <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100" style="width: 10%">
                            +10µs
                        </div>
                        <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 90%">
                            {{ test.time }}µs
                        </div>
                    </div>
                </td>
                <td><strong>{{ name }}</strong></td>
            </tr>
            <tr>
                <td colspan="2">{{ test.details|nl2br }}</td>
            </tr>
            {% endfor %}
        </table>
    </div>